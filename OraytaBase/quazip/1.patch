+    QIODevice *iodevice = reinterpret_cast<QIODevice*>(stream);
+    uLong ret;
+    qint64 ret64 = iodevice->write((char*)buf,size);
+    if (ret64 != -1) {
+        d->pos += ret64;
+    }
+    ret = (uLong) ret64;
+    return ret;
+}
