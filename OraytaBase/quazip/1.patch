+    QIODevice *iodevice = reinterpret_cast<QIODevice*>(stream);
+    uLong ret;
+    qint64 ret64;
+    if (iodevice->isSequential()) {
+        ret64 = d->pos;
+    } else {
+        ret64 = iodevice->pos();
+    }
+    ret = static_cast<uLong>(ret64);
+    return ret;
+}
+
+ZPOS64_T ZCALLBACK qiodevice64_tell_file_func (
